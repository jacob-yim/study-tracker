{
  "name": "Study Tracker",
  "version": "1.0",
  "description": "An extension that reminds you to take study breaks",
  "manifest_version": 2,
  "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Study Tracker",
      "default_icon": "clock.png"
    },
  "background": {
      "scripts": ["background.js"]
    },
  "icons": {
      "128": "clock.png"
    }
  }
  
